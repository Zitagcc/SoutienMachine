package tp4q2;

public class Main {
    public static void main(String[] args) {
        Cat chat = new Cat("Bishou");

        // Héritage de la classe Animal + exécution d'un thread via Runnable
        chat.eat(); // Méthode héritée de la classe Animal
        Thread thread = new Thread(chat); // Exécuter le thread
        thread.start();
    }
}